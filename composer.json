{
	"name": "gathernholding/yii-bootstrap-bower",
    "description": "Bower Bootstrap",
    "require": {
        "php": ">=8.1"
    },
  "extra": {
    "installer-paths": {
      "bower/": ["gathernholding/yii-bootstrap-bower"]
    }
  },
  "require-dev": {
	  "phpunit/phpunit": ">=4.0"
  },
"extra": {
    "asset-installer-paths": {
	"npm-asset-library": "vendor/npm",
	"bower-asset-library": "vendor/bower"
	},
	"branch-alias": {
	    "dev-master": "1.0-dev"
	}
    }
}
